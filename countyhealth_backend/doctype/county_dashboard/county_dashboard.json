{
 "doctype": "DocType",
 "name": "CountyDashboard",
 "module": "countyhealth_backend",
 "custom": 0,
 "autoname": "prompt",
 "title_field": "county_name",
 "search_fields": "county_name,login_username",
 "sort_field": "county_name",
 "sort_order": "ASC",
 "track_changes": 1,
 "track_seen": 1,
 "fields": [
  {
   "fieldname": "basic_info_section",
   "fieldtype": "Section Break",
   "label": "Basic Information"
  },
  {
   "fieldname": "county_name",
   "fieldtype": "Data",
   "label": "County Name",
   "reqd": 1,
   "unique": 1,
   "in_list_view": 1,
   "in_standard_filter": 1,
   "description": "Display name for the county"
  },
  {
   "fieldname": "dashboard_url",
   "fieldtype": "Data",
   "label": "Dashboard URL",
   "reqd": 1,
   "options": "URL",
   "in_list_view": 1,
   "description": "URL to the county's dashboard"
  },
  {
   "fieldname": "login_section",
   "fieldtype": "Section Break",
   "label": "Login Credentials",
   "collapsible": 1
  },
  {
   "fieldname": "login_username",
   "fieldtype": "Data",
   "label": "Login Username",
   "reqd": 1,
   "unique": 1,
   "in_list_view": 1,
   "in_standard_filter": 1,
   "description": "Username for county login (lowercase letters, numbers, underscores only)"
  },
  {
   "fieldname": "login_password",
   "fieldtype": "Password",
   "label": "Login Password",
   "reqd": 1,
   "description": "Password for county login (minimum 8 characters)"
  },
  {
   "fieldname": "status_section",
   "fieldtype": "Section Break",
   "label": "Status",
   "collapsible": 1
  },
  {
   "fieldname": "is_active",
   "fieldtype": "Check",
   "label": "Is Active",
   "default": 1,
   "in_list_view": 1,
   "description": "Uncheck to disable this county's login"
  },
  {
   "fieldname": "last_login",
   "fieldtype": "Datetime",
   "label": "Last Login",
   "read_only": 1,
   "description": "Timestamp of last successful login"
  }
 ],
 "permissions": [
  {
   "role": "System Manager",
   "read": 1,
   "write": 1,
   "create": 1,
   "delete": 1,
   "export": 1,
   "import": 1,
   "report": 1
  },
  {
   "role": "Administrator",
   "read": 1,
   "write": 1,
   "create": 1,
   "delete": 1,
   "export": 1,
   "import": 1,
   "report": 1
  }
 ],
 "engine": "InnoDB",
 "row_format": "Dynamic",
 "naming_rule": "Set by user",
 "allow_import": 1,
 "allow_export": 1,
 "show_title_field_in_link": 1
}
